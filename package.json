    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^6.7.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "postcss-loader": "^1.1.1",
    "style-loader": "^0.13.0",
